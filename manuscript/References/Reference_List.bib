	title = {Maximum extent of {Late} {Pleistocene} glaciers and last deglaciation of {La} {Cerdanya} mountains, {Southeastern} {Pyrenees}},
	volume = {231},
	issn = {0169-555X},
	url = {http://www.sciencedirect.com/science/article/pii/S0169555X14005984},
